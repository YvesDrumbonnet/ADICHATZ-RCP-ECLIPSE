    <!-- ****************************************** -->
    <!--		Q U E R Y - R E F E R E N C E 		-->
    <!-- ****************************************** -->	
    <!-- ****************** -->
    <!-- queryReferenceType -->
    <!-- ****************** -->
    <xs:complexType name="queryReferenceType">
  		<xs:complexContent>
			<xs:extension base="elementType">
				<xs:attribute name="adiQueryURI" type="xs:string" use="required"/>
				<xs:attribute name="preferenceURI" type="xs:string"/>
			</xs:extension>
		</xs:complexContent>
     </xs:complexType>

    <!-- ****************************	-->
    <!--			Q U E R Y			-->
    <!-- ****************************	-->
    <!-- ********************** -->
    <!-- native content provider -->
    <!-- ********************** -->
   <xs:complexType  name="nativeContentProviderType">
		<xs:complexContent>
			<xs:extension base="contentProviderType">
				<xs:attribute name="fieldName" type="xs:string"/>	
			</xs:extension>
		</xs:complexContent>
   </xs:complexType>

    <!-- ******************************	-->
    <!--			W I D G E T			-->
    <!-- ******************************	-->
	<xs:complexType name="widgetType">
		<xs:complexContent>
			<xs:extension base="validElementType">
				<xs:sequence minOccurs="0">
					<xs:choice>
						<xs:element name="listeners" type="listenersType"/>
						<xs:element name="accessibilities" type="accessibilitiesType"/>
						<xs:element name="additionalCode" type="xs:string"/>
					</xs:choice>
				</xs:sequence>
				<xs:attribute name="controllerClassName" type="xs:NCName"/>
				<!-- SWT attributes -->	        
	            <xs:attribute name="enabled" type="xs:string"/>
				<xs:attribute name="style" type="xs:string"/>
				<xs:attribute name="toolTipText" type="xs:string"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
    <!-- *******************************	-->
    <!--			C O N T R O L			-->
    <!-- *******************************	-->
	<xs:complexType name="controlType">
		<xs:complexContent>
			<xs:extension base="widgetType">
				<xs:attribute name="background" type="xs:string"/>
				<xs:attribute name="backgroundImage" type="xs:string"/>
				<xs:attribute name="bounds" type="xs:string"/>
				<xs:attribute name="capture" type="xs:string"/>
				<xs:attribute name="foreground" type="xs:string"/>
				<xs:attribute name="focus" type="xs:string"/>
				<xs:attribute name="font" type="xs:string"/>
				<xs:attribute name="layoutData" type="xs:string"/>
				<xs:attribute name="location" type="xs:string"/>
				<xs:attribute name="menu" type="xs:string"/>
				<xs:attribute name="redraw" type="xs:string"/>
				<xs:attribute name="size" type="xs:string"/>
				<xs:attribute name="visible" type="xs:string"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
    <!-- *******************************************	-->
    <!--			H E L P - B U T T O N				-->
    <!-- *******************************************	-->
	<xs:complexType name="helpButtonType">
		<xs:complexContent>
			<xs:extension base="controlType">
        		<xs:attribute name="image" type="xs:string"/>
        		<xs:attribute name="helpLabel" type="xs:string" use="required"/>
        		<xs:attribute name="helpSpecify" type="xs:string" use="required"/>
        		<xs:attribute name="helpMessage" type="xs:string" use="required"/>
 			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
    <!-- ***********************************************	-->
    <!--			C O N T R O L -  F I E L D				-->
    <!-- ***********************************************	-->
	<xs:complexType name="controlFieldType">
		<xs:complexContent>
			<xs:extension base="controlType">
				<xs:choice>
					<xs:element name="validators" type="validatorsType"/>
					<xs:element name="initialValue" type="xs:string"/>
					<xs:element name="convertModelToTarget" type="xs:string"/>
					<xs:element name="convertTargetToModel" type="xs:string"/>
					<xs:element name="menuManager" type="menuManagerType"/>
				</xs:choice>
				<xs:attribute name="property" type="xs:NCName"/>
				<xs:attribute name="controlValueType" type="xs:NCName"/><!-- For specifying a type for the value -->
				<xs:attribute name="forceBinding" type="xs:NCName"/><!-- Force binding service even if property is null ==> fire validators -->
            	<xs:attribute name="labelBackground" type="xs:string"/>
            	<xs:attribute name="labelFont" type="xs:string"/>
            	<xs:attribute name="labelForeground" type="xs:string"/>
            	<xs:attribute name="labelLayoutData" type="xs:string"/>
            	<xs:attribute name="labelText" type="xs:string"/>
				<xs:attribute name="lazyFetches" type="xs:string"/>
            	<xs:attribute name="linkedControl" type="xs:string"/>
            	<xs:attribute name="mandatory" type="xs:boolean"/>
            	<xs:attribute name="noLabel" type="xs:boolean"/>
				<xs:attribute name="fieldBindManagerClassName" type="xs:NCName"/>
 			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
