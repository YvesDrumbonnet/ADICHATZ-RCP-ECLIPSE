    <!-- buttonBar -->
    <!-- ********* -->
    <xs:complexType name="buttonBarType">
		<xs:complexContent>
			<xs:extension base="collectionType">
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element name="button" type="buttonType"/>
					<xs:element name="separator" type="separatorType"/>
				</xs:choice>
	    	    <xs:attribute name="separator" type="xs:boolean"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

    <!-- **********************************	-->
    <!--			C O N T R O L S			-->
    <!-- **********************************	-->
    <!-- ****** -->
    <!-- action -->
    <!-- ****** -->
    <xs:complexType name="actionType">
 		<xs:complexContent>
			<xs:extension base="widgetType">
