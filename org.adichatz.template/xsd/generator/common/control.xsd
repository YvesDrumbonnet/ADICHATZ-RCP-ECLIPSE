				<xs:attribute name="rank" type="xs:int"/>
				<xs:attribute name="text" type="xs:string"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

    <!-- ********** -->
    <!-- MenuAction -->
    <!-- ********** -->
    <xs:complexType name="menuActionType">
 		<xs:complexContent>
			<xs:extension base="widgetType">
				<xs:sequence>
					<xs:element name="params" type="paramsType"/>
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:element name="action" type="actionType"/>
						<xs:element name="menuAction" type="menuActionType"/>
						<xs:element name="separator" type="separatorType"/>
					</xs:choice>
				</xs:sequence>
				<xs:attribute name="imageDescriptor" type="xs:string"/>
            	<xs:attribute name="reflow" type="xs:boolean"/>
				<xs:attribute name="text" type="xs:string"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

    <!-- ********* -->
