    <!-- ************** -->
    <!-- managedToolBar -->
    <!-- ************** -->
    <xs:complexType name="managedToolBarType">
		<xs:complexContent>
			<xs:extension base="collectionType">
				<xs:choice minOccurs="0" maxOccurs="unbounded">
					<xs:element name="action" type="actionType"/>
					<xs:element name="menuAction" type="menuActionType"/>
					<xs:element name="separator" type="separatorType"/>
				</xs:choice>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>	

    <!-- ******* -->
    <!-- toolBar -->
    <!-- ******* -->
    <xs:complexType name="toolBarType">
		<xs:complexContent>
			<xs:extension base="collectionType">
				<xs:sequence>
					<xs:element name="layout" type="layoutType"/>
					<xs:choice minOccurs="0" maxOccurs="unbounded">
						<xs:element name="toolItem" type="toolItemType"/>
						<xs:element name="separator" type="separatorType"/>
					</xs:choice>
				</xs:sequence>
				<xs:attribute name="layoutData" type="xs:string"/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
